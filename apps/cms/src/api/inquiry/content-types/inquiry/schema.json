{
  "kind": "collectionType",
  "collectionName": "inquiries",
  "info": {
    "singularName": "inquiry",
    "pluralName": "inquiries",
    "displayName": "Inquiry"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "phone": {
      "type": "string"
    },
    "organization": {
      "type": "string"
    },
    "message": {
      "type": "text"
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "employer",
        "landlord"
      ]
    },
    "currentStatus": {
      "type": "enumeration",
      "default": "new",
      "enum": [
        "new",
        "contacted",
        "resolved"
      ]
    }
  }
}
