{
  "kind": "collectionType",
  "collectionName": "tags",
  "info": {
    "singularName": "tag",
    "pluralName": "tags",
    "displayName": "Tag"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "identifier": {
      "type": "uid",
      "required": true
    },
    "pageLabel": {
      "type": "string",
      "required": true
    },
    "sectionLabel": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string",
      "required": true
    },
    "tags": {
      "type": "text",
      "required": true,
      "regex": "^[a-z]+(?: [a-z]+)*$"
    },
    "href": {
      "type": "string",
      "required": true
    }
  }
}
